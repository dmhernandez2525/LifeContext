# LifeContext Mobile Environment Variables
# Copy this file to .env.local and fill in your values

# Convex Backend URL (required for cloud sync)
EXPO_PUBLIC_CONVEX_URL=

# Optional: Anthropic Claude API Key (for AI features)
# Note: Can also be configured in-app via Settings
EXPO_PUBLIC_CLAUDE_API_KEY=

# Optional: OpenAI Whisper API Key (for transcription)
# Note: Can also be configured in-app via Settings
EXPO_PUBLIC_OPENAI_API_KEY=
